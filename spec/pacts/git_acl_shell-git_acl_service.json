{
  "consumer": {
    "name": "Git ACL Shell"
  },
  "provider": {
    "name": "Git ACL Service"
  },
  "interactions": [
    {
      "description": "a permission request",
      "provider_state": "some-key-id is allowed to access alias-for-the-real-repo",
      "request": {
        "method": "get",
        "path": "/permission",
        "query": "keyId=some-key-id&alias=alias-for-the-real-repo"
      },
      "response": {
        "status": 200,
        "headers": {
        }
      }
    },
    {
      "description": "a permission request",
      "provider_state": "some-key-id is not allowed to access alias-for-the-real-repo",
      "request": {
        "method": "get",
        "path": "/permission",
        "query": "keyId=some-key-id&alias=alias-for-the-real-repo"
      },
      "response": {
        "status": 403,
        "headers": {
        }
      }
    },
    {
      "description": "a real repo name lookup by alias",
      "provider_state": "alias is an alias of real-name",
      "request": {
        "method": "get",
        "path": "/real-name",
        "query": "alias=alias-for-the-real-repo"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "text/plain; charset=utf-8"
        },
        "body": "the-real-repo"
      }
    },
    {
      "description": "a real repo name lookup by alias",
      "provider_state": "an unknown alias",
      "request": {
        "method": "get",
        "path": "/real-name",
        "query": "alias=unknown-alias"
      },
      "response": {
        "status": 404,
        "headers": {
        }
      }
    }
  ],
  "metadata": {
    "pactSpecificationVersion": "1.0.0"
  }
}